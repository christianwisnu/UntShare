<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/md_amber_600</item>
        <item name="colorPrimaryDark">@color/md_amber_600</item>
        <item name="colorAccent">@color/md_amber_900</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_in_down</item>
        <item name="android:windowExitAnimation">@anim/slide_out_top</item>
    </style>

    <style name="ExpandedAppBar" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">28sp</item>
        <item name="android:textColor">#000</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="CollapsedAppBar" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">26sp</item>
        <item name="android:textColor">#000</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="MsgDialogList" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:background">#99000000</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLines">2</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">10dp</item>
        <item name="android:typeface">sans</item>
    </style>


    <style name="text_large_bold">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/text_margin</item>
        <item name="android:layout_marginRight">@dimen/text_margin</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:textStyle">bold</item>
        <item name="android:text">"Large Bold"</item>
    </style>

    <style name="text_medium_bold">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/text_margin</item>
        <item name="android:layout_marginRight">@dimen/text_margin</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:text">"Medium Bold"</item>
    </style>

    <style name="text_large_normal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/text_margin</item>
        <item name="android:layout_marginRight">@dimen/text_margin</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:textStyle">normal</item>
        <item name="android:text">"Large Normal"</item>
    </style>

    <style name="text_medium_normal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/text_margin</item>
        <item name="android:layout_marginRight">@dimen/text_margin</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:text">"Medium Normal"</item>
    </style>

    <style name="text_small_bold">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/text_margin</item>
        <item name="android:layout_marginRight">@dimen/text_margin</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:textStyle">bold</item>
        <item name="android:text">"Small Bold"</item>
    </style>

    <style name="text_small_normal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/text_margin</item>
        <item name="android:layout_marginRight">@dimen/text_margin</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:textStyle">normal</item>
        <item name="android:text">"Medium Normal"</item>
    </style>
    <style name="image_cyle_add_bottom">
        <item name="android:background">@drawable/slct_orange_to_white</item>
        <item name="android:layout_gravity">right|bottom</item>
        <item name="android:layout_width">@dimen/hw_img_cycle_bottom</item>
        <item name="android:layout_height">@dimen/hw_img_cycle_bottom</item>
        <item name="android:layout_margin">@dimen/second_margin</item>
        <item name="android:scaleType">centerInside</item>
    </style>

    <style name="ViewMessageText">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="text_medium_normal_nomargin_lef_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/text_margin</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:text">"Medium Normal"</item>
    </style>

    <style name="button_small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/text_margin</item>
        <item name="android:layout_marginRight">@dimen/text_margin</item>
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:buttonStyle">?android:attr/buttonStyleSmall</item>
        <item name="android:textStyle">normal</item>
        <item name="android:text">"Medium Normal"</item>
    </style>

    ""
    <style name="listview">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:divider">#000000</item>
    </style>

    <style name="ImgBack">
        <item name="android:layout_width">@dimen/actionbar_dimen</item>
        <item name="android:layout_height">@dimen/actionbar_dimen</item>
        <item name="android:layout_margin">@dimen/MarginLayout</item>
        <item name="android:background">@drawable/slctcycleorange</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:src">@mipmap/ic_action_goleft</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
